;   param=value ; assign values to parameters

config_version=4

[application]

config/name="personal project"
run/main_scene="res://Game.tscn"
config/icon="res://icon.png"

[autoload]

JsonData="*res://JSONData.gd"
Actions="*res://Global/Actions.gd"
State="*res://Global/State.gd"
Ply="*res://Global/Player.gd"
Core="*res://Global/core.gd"
